{
  "scripts": {
    "lint": "node node_modules/eslint/bin/eslint.js . cypress --ignore-pattern \"cache/\"",
    "lint:fix": "node node_modules/eslint/bin/eslint.js . cypress --fix --ignore-pattern \"cache/\"",
    "test": "cypress run",
    "open": "cypress open --e2e",
    "cypress:run": "cypress run",
    "cypress:report:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/report.json",
    "cypress:report:generate": "marge cypress/reports/report.json -f report -o cypress/reports/html",
    "cypress:report": "npm run cypress:run && npm run cypress:report:merge && npm run cypress:report:generate"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@cypress/code-coverage": "^3.10.0",
    "@eslint/js": "^8.57.1",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "cypress": "^13.6.6",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.36.1",
    "globals": "^15.9.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.2.0"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@trpc/server": "^10.45.1",
    "faker": "^5.5.3",
    "glob": "^11.0.0",
    "lru-cache": "^7.1.3",
    "node": "^19.8.1",
    "socket.io-client": "^4.7.3",
    "string-interpolation-js": "^1.0.8"
  }
}
